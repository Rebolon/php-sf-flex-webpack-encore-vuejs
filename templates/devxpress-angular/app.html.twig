{% extends 'spa.html.twig' %}

{% block head %}
    <meta charset="utf-8">
    <base href="/demo/devxpress-angular">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="favicon.ico">

    <link rel="stylesheet" type="text/css" href="/dist-ng/devxpress/dx.common.css" />
    <link rel="stylesheet" type="text/css" href="/dist-ng/devxpress/dx.spa.css" />

    <link rel="dx-theme" data-theme="android5.light" href="/dist-ng/devxpress/dx.android5.light.css" />
    <link rel="dx-theme" data-theme="ios7.default" href="/dist-ng/devxpress/dx.ios7.default.css" />
    <link rel="dx-theme" data-theme="win10.black" href="/dist-ng/devxpress/dx.win10.black.css" data-active="true" />
    <link rel="dx-theme" data-theme="win10.white" href="/dist-ng/devxpress/dx.win10.white.css" data-active="false" />
    <link rel="dx-theme" data-theme="generic.light" href="/dist-ng/devxpress/dx.light.css" data-active="true" />
    <link rel="dx-theme" data-theme="generic.dark" href="/dist-ng/devxpress/dx.dark.css" data-active="false" />
    <link rel="dx-theme" data-theme="generic.light.compact" href="/dist-ng/devxpress/dx.light.compact.css" data-active="false" />
    <link rel="dx-theme" data-theme="generic.dark.compact" href="/dist-ng/devxpress/dx.dark.compact.css" data-active="false" />
    <link rel="dx-theme" data-theme="generic.contrast" href="/dist-ng/devxpress/dx.contrast.css" data-active="false" />
    <link rel="dx-theme" data-theme="generic.contrast.compact" href="/dist-ng/devxpress/dx.contrast.compact.css" data-active="false" />

    {{ parent() }}
{% endblock %}

{% block stylesheets %}
<link rel="stylesheet" type="text/css" href="{{ asset('dist-ng/devxpress/dx.common.css') }}" />
<link rel="stylesheet" type="text/css" href="{{ asset('dist-ng/devxpress/dx.spa.css') }}" />

<link rel="dx-theme" data-theme="android5.light" href="{{ asset('dist-ng/devxpress/dx.android5.light.css') }}" />
<link rel="dx-theme" data-theme="ios7.default" href="{{ asset('dist-ng/devxpress/dx.ios7.default.css') }}" />
<link rel="dx-theme" data-theme="win10.black" href="{{ asset('dist-ng/devxpress/dx.win10.black.css') }}" data-active="true" />
<link rel="dx-theme" data-theme="win10.white" href="{{ asset('dist-ng/devxpress/dx.win10.white.css') }}" data-active="false" />
<link rel="dx-theme" data-theme="generic.light" href="{{ asset('dist-ng/devxpress/dx.light.css') }}" data-active="true" />
<link rel="dx-theme" data-theme="generic.dark" href="{{ asset('dist-ng/devxpress/dx.dark.css') }}" data-active="false" />
<link rel="dx-theme" data-theme="generic.light.compact" href="{{ asset('dist-ng/devxpress/dx.light.compact.css') }}" data-active="false" />
<link rel="dx-theme" data-theme="generic.dark.compact" href="{{ asset('dist-ng/devxpress/dx.dark.compact.css') }}" data-active="false" />
<link rel="dx-theme" data-theme="generic.contrast" href="{{ asset('dist-ng/devxpress/dx.contrast.css') }}" data-active="false" />
<link rel="dx-theme" data-theme="generic.contrast.compact" href="{{ asset('dist-ng/devxpress/dx.contrast.compact.css') }}" data-active="false" />

<link rel="stylesheet" href="{{ asset('dist-ng/devxpress/styles.bundle.css') }}">

<!-- preload JS -->
<link rel="preload" href="{{ asset('dist-ng/devxpress/inline.bundle.js') }}" as="script"/>
<link rel="preload" href="{{ asset("dist-ng/devxpress/polyfills.bundle.js") }}" as="script"/>
<link rel="preload" href="{{ asset("dist-ng/devxpress/vendor.bundle.js") }}" as="script"/>
<link rel="preload" href="{{ asset("dist-ng/devxpress/main.bundle.js") }}" as="script"/>
<link rel="preload" href="{{ asset('dist/manifest.js') }}" as="script"/>
<link rel="preload" href="{{ asset("dist/vendor.js") }}" as="script"/>
{% endblock %}

{% block body %}
    <app-root></app-root>
{% endblock %}

{% block javascripts %}
    <!-- HardCoded part whereas it should be generated with ngcli build system: it should be better to serve the  -->
    <script type="text/javascript" src="/dist-ng/devxpress/inline.bundle.js"></script>
    <script type="text/javascript" src="/dist-ng/devxpress/polyfills.bundle.js"></script><!-- generate Zone already loaded. Error -->
    <script type="text/javascript" src="/dist-ng/devxpress/vendor.bundle.js"></script>
    <script type="text/javascript" src="/dist-ng/devxpress/main.bundle.js"></script>
{%  endblock %}
